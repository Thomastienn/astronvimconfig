{

        "diff": {
            "prefix": ["diff"],
            "body": [
                "#!/bin/bash",
				"",
				"MAIN=\"{{MAIN}}\"",
				"BRUTE=\"{{BRUTE}}\"",
				"GEN=\"{{GEN}}\"",
				"",
				"if [[ ! -f \"$MAIN\" ]]; then",
				"    echo \"Error: $MAIN not found\"",
				"    exit 1",
				"fi",
				"",
				"if [[ ! -f \"$BRUTE\" ]]; then",
				"    echo \"Error: $BRUTE not found\"",
				"    exit 1",
				"fi",
				"",
				"if [[ ! -f \"$GEN\" ]]; then",
				"    echo \"Error: $GEN not found\"",
				"    exit 1",
				"fi",
				"",
				"i=1",
				"while true; do",
				"    printf \"\\rTest %d\" \"$i\"",
				"    ",
				"    if ! python3 \"$GEN\" > /tmp/input.txt 2>/tmp/gen_err.txt; then",
				"        echo \"\"",
				"        echo \"Error: generator failed on test $i\"",
				"        cat /tmp/gen_err.txt",
				"        exit 1",
				"    fi",
				"    ",
				"    if ! python3 \"$MAIN\" < /tmp/input.txt > /tmp/out_main.txt 2>/tmp/main_err.txt; then",
				"        echo \"\"",
				"        echo \"Error: main solution crashed on test $i\"",
				"        echo \"--- INPUT ---\"",
				"        cat /tmp/input.txt",
				"        echo \"--- ERROR ---\"",
				"        cat /tmp/main_err.txt",
				"        exit 1",
				"    fi",
				"    ",
				"    if ! python3 \"$BRUTE\" < /tmp/input.txt > /tmp/out_brute.txt 2>/tmp/brute_err.txt; then",
				"        echo \"\"",
				"        echo \"Error: brute solution crashed on test $i\"",
				"        echo \"--- INPUT ---\"",
				"        cat /tmp/input.txt",
				"        echo \"--- ERROR ---\"",
				"        cat /tmp/brute_err.txt",
				"        exit 1",
				"    fi",
				"    ",
				"    if ! diff -q /tmp/out_main.txt /tmp/out_brute.txt > /dev/null; then",
				"        echo \"\"",
				"        echo \"========================\"",
				"        echo \"MISMATCH on test $i\"",
				"        echo \"========================\"",
				"        echo \"\"",
				"        echo \"--- INPUT ---\"",
				"        cat /tmp/input.txt",
				"        echo \"\"",
				"        echo \"--- YOUR SOLUTION ---\"",
				"        cat /tmp/out_main.txt",
				"        echo \"\"",
				"        echo \"--- BRUTE SOLUTION ---\"",
				"        cat /tmp/out_brute.txt",
				"        exit 1",
				"    fi",
				"    ((i++))",
				"done"

            ],
            "description": "Desciption"
        }
    
}
